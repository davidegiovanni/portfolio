{
  "name": "theme-default",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix",
    "dev": "remix dev -c \"run-p dev:*\"",
    "dev:mocks": "node ./mocks/index.js",
    "dev:css": "tailwindcss -i ./tailwind.css -o ./app/styles/tailwind.css --watch",
    "dev:remix": "remix-serve ./build/index.js",
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./tailwind.css -o ./app/styles/tailwind.css --minify",
    "build:remix": "remix build",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc -b",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mswjs/http-middleware": "^0.8.0",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/themes": "^2.0.3",
    "@remix-run/css-bundle": "^2.0.1",
    "@remix-run/node": "^2.0.1",
    "@remix-run/react": "^2.0.1",
    "@remix-run/serve": "^2.0.1",
    "@uidotdev/usehooks": "^2.4.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^11.0.3",
    "isbot": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@remix-run/dev": "^2.0.1",
    "@remix-run/eslint-config": "^2.0.1",
    "@remix-run/testing": "^2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "binode": "^1.0.5",
    "c8": "^8.0.1",
    "cssnano": "^6.0.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "happy-dom": "^12.8.0",
    "msw": "^1.3.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.10",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
